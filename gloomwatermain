local Tab2 = Window:CreateTab("Tools", 0) -- Title, Image
local Section2 = Tab2:CreateSection("Steal all Tools")
local Button14 = Tab2:CreateButton({
	Name = "Steal all tools",
	Callback = function()
	local descendants = game:GetDescendants()
	for index, descendant in pairs(descendants) do
	    if descendant:IsA("Tool") then
	        descendant.Parent = game.Players.LocalPlayer.Backpack
	    end
	end
	end,
})
local Button15 = Tab2:CreateButton({
	Name = "Shoot any equipped gun (must be equipped)",
	Callback = function()
	local args = {
    [1] = Vector3.new(327.12213134765625, 3.6121790409088135, -77.840087890625)
}

game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Events.FireEvent:FireServer(unpack(args))
	end,
})
local Button16 = Tab2:CreateButton({
	Name = "Reload any equipped gun (must be equipped)",
	Callback = function()
	local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Events.Reload:FireServer(unpack(args))

	end,
})
local Tab3 = Window:CreateTab("Cop stuff", 0) -- Title, Image
local Input = Tab3:CreateInput({
	Name = "Grab (have grab tool equipped, 50% chance it works)",
	PlaceholderText = "Input name of the player",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
local args = {
    [1] = "Cuff",
    [2] = game:GetService("Players")[(Text)].Character.LeftUpperArm
}

game:GetService("Players").LocalPlayer.Character.Grab.RemoteEvent:FireServer(unpack(args))
	end,
})
local Input2 = Tab3:CreateInput({
	Name = "Ungrab (have grab tool equipped, 50% chance it works)",
	PlaceholderText = "Input name of the player",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
local args = {
    [1] = "Uncuff",
    [2] = game:GetService("Players")[(Text)].Character.LeftUpperArm
}

game:GetService("Players").LocalPlayer.Character.Grab.RemoteEvent:FireServer(unpack(args))
	end,
})
