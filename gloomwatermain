local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
	Name = "6fr Hub",
	LoadingTitle = "Loading HUB",
	LoadingSubtitle = "by 6",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = "LEP: Gloomwater", -- Create a custom folder for your hub/game
		FileName = "6fr Hub"
	},
        Discord = {
        	Enabled = false,
        	Invite = "sirius", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = false -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
	}
	
})
local Tab = Window:CreateTab("Remotes", 0) -- Title, Image
local Section = Tab:CreateSection("Toggle NPCs")
local Button = Tab:CreateButton({
	Name = "Toggle Clothing Store",
	Callback = function()
		local args = {
    [1] = workspace.NPCS.ClothingStore
}

game:GetService("ReplicatedStorage").Events.NPC:FireServer(unpack(args))
	end,
})
local Button2 = Tab:CreateButton({
	Name = "Toggle Job Center",
	Callback = function()
	local args = {
